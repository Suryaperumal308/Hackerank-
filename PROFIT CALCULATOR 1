import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int c=sc.nextInt();
        int d=sc.nextInt();
        int e=sc.nextInt();
        float p=(float)(c*d)-(e*c+100);
        System.out.println("Number of copies sold:"+c);
        System.out.println("Cost of each copy:"+d);
        System.out.println("Cost spent by agency on each newspaper:"+e);
        System.out.printf("The profit obtained is Rs.%.2f",p);
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
